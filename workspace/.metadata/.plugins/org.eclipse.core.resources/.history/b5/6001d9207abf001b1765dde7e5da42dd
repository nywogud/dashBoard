package com.jhl.dashBoard.controller;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.CrossOrigin;
import org.springframework.web.bind.annotation.RequestMapping;

import com.jhl.dashBoard.service.DataService;


@CrossOrigin
@Controller
public class homeController {
	
	@Autowired
	DataService dataService;
	
	@RequestMapping("/")
	public String home() {
		try {
			
			for(int i=0; i<dataService.getData().size(); i++) {
				System.out.println("데이터" + dataService.getData().get(i).getTestNumber());
				System.out.println("데이터" + dataService.getData().get(i).getMethod());
				System.out.println("데이터" + dataService.getData().get(i).getResult());
			}
			
			
		}
		catch(Exception e) {
			System.out.println("오류 : " + e);
		}
		
//		ModelAndView mav = new ModelAndView();
//		mav.setViewName("home");
		
		return "home";
	}

}
